{
  "name": "@umijs/plugin-qiankun",
  "version": "1.2.2",
  "description": "umi plugin for qiankun",
  "repository": "umijs/umi-plugin-qiankun",
  "keywords": [
    "umi-plugin",
    "micro frontend",
    "qiankun"
  ],
  "authors": [
    {
      "name": "chencheng",
      "email": "sorrycc@gmail.com"
    }
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "start": "concurrently \"npm run start:master\" \"npm run start:app1\" \"npm run start:app2\"",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "npm run lint:js && npm run lint:prettier && npm test",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:prettier": "check-prettier lint",
    "prettier": "prettier -c --write **/*",
    "prepublishOnly": "npm run build",
    "build": "father-build",
    "release": "np --no-cleanup --yolo --no-publish",
    "start:master": "cd examples/master && umi dev",
    "start:app1": "cd examples/app1 && umi dev",
    "start:app2": "cd examples/app2 && umi dev",
    "test": "umi-test"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "qiankun": "^1.2.1"
  },
  "peerDependencies": {
    "react": "16",
    "react-dom": "16",
    "webpack": "^4.41.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/react-dom": "^16.8.4",
    "@umijs/fabric": "^1.1.10",
    "check-prettier": "^1.0.3",
    "concurrently": "^4.1.2",
    "coveralls": "3",
    "father-build": "^1.2.0",
    "husky": "1",
    "lint-staged": "8",
    "mockjs": "^1.0.1-beta3",
    "np": "5",
    "prettier": "1",
    "umi": "^2.8.20",
    "umi-plugin-react": "^1.9.13",
    "umi-request": "^1.1.0",
    "umi-test": "^1.8.2",
    "umi-types": "^0.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,json,css,md}": [
      "npm run prettier",
      "git add"
    ],
    "**/*.{js,ts}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "files": [
    "lib",
    "master.js",
    "slave.js"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
