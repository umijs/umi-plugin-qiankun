{
  "headers": [
    {
      "source": "**/*.@(jpg|jpeg|gif|png|txt|html|js|css)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Headers", "value": "Origin, X-Requested-With, Content-Type, Accept, Range" }
      ]
    }
  ]
}
